  "main": "index.js",
  "scripts": {
    "build": "npm run clearbuild && npm run typecheck && node build/build-bundle.js",
    "build-tsc": "rimraf ./dist && tsc -p ./src/server/tsconfig.json && tsc -p ./src/client/tsconfig.json",
    "clearbuild": "rimraf -G ./dist",
    "typecheck": "tsc --noEmit -p src/client/tsconfig.json && tsc --noEmit -p src/server/tsconfig.json",
    "watch": "npm run clearbuild && cross-env IS_WATCH_MODE=1 node build/build-bundle.js",
    "doc": "typedoc --entryPointStrategy expand ./src/server --entryPointStrategy expand ./src/client --entryPointStrategy expand ./src/shared --tsconfig tsconfigtypedoc.json",
    "test": "jest"
  },
