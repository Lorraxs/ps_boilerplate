{
  "name": "ps_boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-sv && npm run build-cl",
    "build-sv": "npm run clearbuild-sv && tsc -p ./src/server/tsconfig.json && npm run typecheck-sv && node build/build-bundle-server.js",
    "build-cl": "npm run clearbuild-cl && tsc -p ./src/client/tsconfig.json && npm run typecheck-cl && node build/build-bundle.js",
    "build-tsc": "rimraf ./dist && tsc -p ./src/server/tsconfig.json && tsc -p ./src/client/tsconfig.json",
    "client-tsc": "rimraf ./dist/client-tsc && tsc -p ./src/client/tsconfig.json",
    "server-tsc": "rimraf ./dist/server-tsc && tsc -p ./src/server/tsconfig.json",
    "shared-tsc": "rimraf ./dist/shared && tsc -p ./src/shared/tsconfig.json",
    "clearbuild-sv": "rimraf -G ./dist/server",
    "clearbuild-cl": "rimraf -G ./dist/client",
    "typecheck": "tsc --noEmit -p src/client/tsconfig.json && tsc --noEmit -p src/server/tsconfig.json",
    "typecheck-sv": "tsc --noEmit -p src/server/tsconfig.json",
    "typecheck-cl": "tsc --noEmit -p src/client/tsconfig.json",
    "watch": "npm run clearbuild && cross-env IS_WATCH_MODE=1 node build/build-bundle.js",
    "doc": "typedoc --entryPointStrategy expand ./src/server --entryPointStrategy expand ./src/client --entryPointStrategy expand ./src/shared --tsconfig tsconfigtypedoc.json",
    "test": "jest"
  },
  "keywords": [],
  "author": "Lorraxs",
  "license": "ISC",
  "dependencies": {
    "@esbuild-plugins/node-resolve": "^0.2.2",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@overextended/ox_lib": "^3.13.0",
    "@react-spring/web": "^9.6.1",
    "@supabase/supabase-js": "^2.42.4",
    "@types/vec2": "^1.6.2",
    "inversify": "^6.0.1",
    "inversify-props": "^2.2.6",
    "inversify-tracer": "^1.2.2",
    "starboy-framework": "^1.2.2",
    "tiny-glob": "^0.2.9",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@citizenfx/client": "^2.0.6285-1",
    "@citizenfx/server": "^2.0.6285-1",
    "@types/deep-diff": "^1.0.2",
    "@types/lodash": "^4.14.191",
    "@types/node": "^16.18.18",
    "@types/polygon": "^1.0.1",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.15",
    "eslint": "^8.43.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.5"
  }
}
